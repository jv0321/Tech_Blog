
<div class="container d-flex justify-content-center min-vh-100">
  <form class="login-form">
    <h1 style="padding-top: 70px;padding-bottom: 50px">Login</h1>
    <div class="mb-3">
      <label for="username" class="form-label">Username</label>
      <input id="username-login" name="username" type="username" class="form-control">

    </div>
    <div class="mb-3">
      <label class="form-label">Password</label>
      <input id="password-login" name="password" type="password" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary"> <a href="/"></a>Login</button>
    <p style="padding-top: 50px;">Don't have an account? <a href="/register">Register</a></p>
  </form>
</div>

<script src="./js/login.js"></script>